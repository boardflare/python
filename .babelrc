{
    "presets": [
      ["@babel/preset-env", {
        "targets": {
          "browsers": [
            "last 2 Chrome versions",
            "last 2 Safari versions",
            "last 2 Edge versions"
          ]
        },
        "modules": false
      }],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-class-properties"
    ]
}